{
  "name": "example",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest dist --mode production ./src/main.js",
    "test:unit": "vue-cli-service test:unit --require tests/unit/setup.js",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-merge-data": "^0.2.1",
    "vue-router": "^3.3.4",
    "vue-slick-carousel": "^1.0.6",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-e2e-cypress": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-plugin-unit-mocha": "^3.1.1",
    "@vue/cli-service": "^4.2.3",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.1.2",
    "eslint": "^6.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "jsdom-global": "latest",
    "launch-editor-middleware": "^2.2.1",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.0.1",
    "semantic-release": "^17.0.7",
    "sinon": "7.4.1",
    "sinon-chai": "latest",
    "vue-cli-plugin-cz": "~1.2.3",
    "vue-cli-plugin-styleguidist": "^4.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all"
  }
}